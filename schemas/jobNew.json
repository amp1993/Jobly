{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companyNew.schema.json",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        },
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "salary": {
        "type": "integer"
      },
      "equity": {
        "type": "integer",
        "default": null
      },
      "company_handle": {
        "type": "string",
        "minLength": 1,
        "maxLength": 10
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "salary",
      "equity",
      "company_handle"
    ]
  }
  